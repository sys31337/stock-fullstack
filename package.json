{
  "name": "ola-gym-desktop",
  "productName": "Ola Spa & Gym",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": ".",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@choc-ui/chakra-autocomplete": "^5.2.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@react-pdf/renderer": "^3.1.12",
    "@swc/plugin-styled-components": "^1.5.70",
    "@tanstack/react-query": "^4.32.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chakra-dayzed-datepicker": "^0.2.9",
    "chakra-react-select": "^4.7.0",
    "dayjs": "^1.11.9",
    "dayzed": "^3.2.3",
    "electron-updater": "^6.1.1",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "formik": "^2.4.2",
    "framer-motion": "^10.15.0",
    "helmet": "^7.1.0",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "joi": "^17.13.1",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-icons": "^4.10.1",
    "react-normalize": "^0.4.4",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.8.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sweetalert2": "^11.10.1",
    "vite-plugin-i18n-hot-reload": "^1.0.4",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
